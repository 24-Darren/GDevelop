						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON3" variable="AideBt" member="yes">
							<label>Aide</label>
							<handler function="OnAideBtClick" entry="EVT_BUTTON" />
