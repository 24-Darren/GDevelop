						<object class="wxPanel" name="ID_PANEL4" variable="Panel4" member="yes">
							<object class="wxFlexGridSizer" variable="FlexGridSizer5" member="no">
								<cols>1</cols>
								<growablecols>0</growablecols>
								<growablerows>1</growablerows>
								<object class="sizeritem">
									<object class="wxFlexGridSizer" variable="FlexGridSizer10" member="no">
										<cols>3</cols>
										<growablecols>0</growablecols>
										<growablerows>0</growablerows>
										<object class="sizeritem">
											<object class="wxStaticText" name="ID_STATICTEXT7" variable="StaticText7" member="yes">
												<label>Games which were edited can have been saved, under the name of gameDumpX.gdg ( where X is a number ).&#x0A;&#x0A;Game Develop can reopen these games.</label>
												<size>424,68</size>
											</object>
											<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<border>5</border>
											<option>1</option>
										</object>
									</object>
									<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
									<option>1</option>
								</object>
