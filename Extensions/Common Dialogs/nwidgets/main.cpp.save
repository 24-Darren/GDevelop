#include "dlib/gui_widgets.h"
#include <sstream>
#include <string>
#include "MessageBox.h"
#include "YesNoMsgBox.h"

using namespace std;
using namespace dlib;

int main()
{
    string result;
    nw::YesNoMsgBox ynbox("Une jolie YesNoMsgBox", "Test1234567891011121314", result);
    ynbox.wait_until_closed();
    
    nw::OpenFile filebox("test", true);
    filebox.

    nw::MsgBox box("Une jolie MsgBox", "Vous avez choisi "+result);
    box.wait_until_closed();
}
